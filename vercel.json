{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    {
      "src": "^/([a-zA-Z0-9_-]+)\\.php$",
      "dest": "/api/$1.php"
    },
    {
      "src": "^/$",
      "dest": "/api/index.php"
    },
    {
      "src": "^/css/(.*)$",
      "dest": "/public/css/$1"
    },
    {
      "src": "^/js/(.*)$",
      "dest": "/public/js/$1"
    },
    {
      "src": "^/img/(.*)$",
      "dest": "/public/img/$1"
    },
    {
      "src": "^/fonts/(.*)$",
      "dest": "/public/fonts/$1"
    },
    {
      "src": "^/robots.txt$",
      "dest": "/public/robots.txt"
    }
  ]
}